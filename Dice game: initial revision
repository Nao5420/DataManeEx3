#include <stdlib.h>
#include <time.h>
#include <stdio.h>
#include <unistd.h>

int main(){
    int n,m;
    srand((unsigned)time(NULL));
    n = rand()%6+1;
    m = rand()%6+1;
    printf("Rolling dice...\n");
    
    printf("Die 1: %d\n",n);
    printf("Die 2: %d\n",m);
    printf("Total value: %d\n",n+m);

    return 0;
}
